    "workbox-webpack-plugin": "4.2.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "lint": "eslint src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "__PLUGIN_REGEX__": "readonly",
      "__COMMIT_HASH__": "readonly"
    },
    "rules": {
      "max-len": [
        "warn",
        120,
        4
      ],
      "semi": [
        "warn",
        "always"
      ],
      "prefer-const": "warn",
      "quotes": [
        "warn",
        "double"
      ],
      "import/no-duplicates": "warn",
      "import/order": [
        "warn",
        {
          "groups": [
            "builtin",
            "external",
            "internal",
            [
              "parent",
              "sibling"
            ],
            "index",
            "unknown"
          ],
          "pathGroups": [
            {
              "pattern": "react",
              "group": "builtin"
            },
            {
              "pattern": "react-**",
              "group": "builtin"
            },
            {
              "pattern": "react-*/**",
              "group": "builtin"
            },
            {
              "pattern": "@ractf/**",
              "group": "external",
              "position": "before"
            },
            {
              "pattern": "{.,..}/**/*.(css|sass|less|scss)",
              "patternOptions": {
                "dot": true,
                "nocomment": true
              },
              "group": "unknown",
              "position": "after"
            },
            {
              "pattern": "**/*.(css|sass|less|scss)",
              "patternOptions": {
                "dot": true,
                "nocomment": true
              },
              "group": "unknown",
              "position": "after"
            }
          ],
          "pathGroupsExcludedImportTypes": [],
          "newlines-between": "always"
        }
      ],
      "no-multiple-empty-lines": [
        "warn",
        {
          "max": 2,
          "maxEOF": 1
        }
      ],
      "import/newline-after-import": [
        "warn",
        {
          "count": 2
        }
      ],
      "eol-last": [
        "error",
        "always"
      ],
      "react/display-name": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
